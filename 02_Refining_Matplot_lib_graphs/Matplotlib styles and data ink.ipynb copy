    "#Pass the ax to the .plot function\n",
    "df.groupby('Continent')['life_expectancy'].median().sort_values().plot(kind='barh', ax=ax, color=['darkred', 'red', 'grey', 'grey', 'grey', 'grey'], linewidth=0, width=0.75)\n",
    "ax.set_ylabel('')\n",
    "#turning on grid, you can also send it option (put ? after the fuction, exmpl. ax.grid, or shift tab)\n",
    "#for colors: http://www.w3schools.com/colors/colors_names.asp\n",
    "\n",
    "\n",
    "#for linestyles '--', '-.', ':'. Here I am just setting axis ticks\n",
    "ax.xaxis.grid(which='major', color='darkgrey', linestyle=':', linewidth=0.5)\n",
    "#turning on the minor grid\n",
    "ax.minorticks_off()\n",
    "ax.xaxis.grid(which='minor', color='darkgrey', linestyle=':', linewidth=0.5)\n",
    "\n",
    "#make sure axis is below\n",
    "ax.set_axisbelow(True)\n",
    "#taking away the line around the bar\n",
    "#linewidth=0\n",
    "#getting rid of the frame\n",
    "ax.spines['top'].set_visible(False)\n",
    "ax.spines['right'].set_visible(False)\n",
    "ax.spines['bottom'].set_visible(False)\n",
    "ax.spines['left'].set_visible(False)\n",
    "\n",
    "#now for the ticks\n",
    "plt.tick_params(\n",
    "    #axis='x',\n",
    "    top='off',\n",
    "    which='major',\n",
    "    left='off',\n",
    "    right='off',\n",
    "    bottom='off',\n",
    "    labeltop='off',\n",
    "    labelbottom='on',\n",
    "    labelright='off')\n",
    "\n",
    "#now for the minor ticks\n",
    "plt.tick_params(\n",
    "    #axis='x',\n",
    "    top='off',\n",
    "    which='minor',\n",
    "    left='off',\n",
    "    right='off',\n",
    "    bottom='off',\n",
    "    labeltop='off',\n",
    "    labelbottom='off')\n",
    "\n",
    "#drawing the line\n",
    "median = df['life_expectancy'].median()\n",
    "ax.plot([median, median], [-4, 10], c='red', linestyle='-')\n",
    "ax.annotate(s='Median life expectancy: 70 years', xy=(72,0), color='red')\n",
    "\n",
    "#removing the 80\n",
    "ax.set_xlim((30,79))"
   ]
  },
